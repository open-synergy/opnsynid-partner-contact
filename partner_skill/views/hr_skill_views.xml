<openerp>
    <data>
        <record id="view_hr_skill_tree" model="ir.ui.view">
            <field name="name">Inherit: Partner Skill Form</field>
            <field name="model">hr.skill</field>
            <field name="inherit_id" ref="hr_skill.view_hr_skill_tree" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='employee_ids']" position="after">
                        <field name="partner_ids" />
                </xpath>
            </field>
        </record>
    </data>
</openerp>
