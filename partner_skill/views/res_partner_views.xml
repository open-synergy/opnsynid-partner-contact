<openerp>
<data>

<record id="res_partner_skill_view_form" model="ir.ui.view">
    <field name="name">res.partner.skill.form</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form" />
    <field name="arch" type="xml">
        <notebook position="inside">
            <page name="skill" string="Skills" attrs="{'invisible':[('is_company', '=', True)]}">
                <field name="skill_ids">
                    <tree editable="top">
                        <field name="skill_id"/>
                        <field name="level_id" domain="[('skill_id','=',skill_id)]"/>
                    </tree>
                </field>
            </page>
        </notebook>
    </field>
</record>

</data>
</openerp>
