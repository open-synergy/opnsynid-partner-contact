<?xml version="1.0" encoding="utf-8" ?>
<openerp>
<data>
<record id="partner_risk_limit_change_request_view_form" model="ir.ui.view">
    <field name="name">Partner Risk Limit Change Request</field>
    <field name="model">partner.risk_limit_change_request</field>
    <field
                name="inherit_id"
                ref="partner_financial_risk_limit_change_request.partner_risk_limit_change_request_view_form"
            />
    <field name="arch" type="xml">
        <xpath expr="//field[@name='risk_account_amount']" position="after">
            <field name="risk_sale_order" />
        </xpath>
    </field>
</record>

<record id="partner_risk_limit_change_request_view_tree" model="ir.ui.view">
    <field name="name">Partner Risk Limit Change Request</field>
    <field name="model">partner.risk_limit_change_request</field>
    <field
                name="inherit_id"
                ref="partner_financial_risk_limit_change_request.partner_risk_limit_change_request_view_tree"
            />
    <field name="arch" type="xml">
        <xpath expr="//field[@name='risk_account_amount']" position="after">
            <field name="risk_sale_order" />
        </xpath>
    </field>
</record>
</data>
</openerp>
